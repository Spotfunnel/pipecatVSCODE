{
    "buildCommand": "npx vite build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/realtime/token",
            "destination": "/api/realtime/token"
        },
        {
            "source": "/api/webhook/test",
            "destination": "/api/webhook/test"
        },
        {
            "source": "/api/recall/launch",
            "destination": "/api/recall/launch"
        },
        {
            "source": "/api/recall/status",
            "destination": "/api/recall/status"
        },
        {
            "source": "/api/recall/stop",
            "destination": "/api/recall/stop"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}